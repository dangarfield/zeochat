    "watchify": "^3.9.0"
  },
  "scripts": {
    "build": "browserify js/main.js > js/dist/bundle.js",
    "install": "npm run build",
    "start": "node web.js",
    "watch": "watchify js/main.js -o js/dist/bundle.js -dv",
