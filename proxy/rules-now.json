{
    "rules": [{
        "pathname": "/ws",
        "dest": "https://zeochat-socket.now.sh"
    }, {
        "pathname": "/classifier",
        "dest": "https://zeochat-classifier.now.sh"
    }, {
        "pathname": "/match**",
        "dest": "https://zeochat-match.now.sh"
    }, {
